{
  "name": "funmooc",
  "version": "0.6.0",
  "description": "Richie-powered CMS for fun-mooc.fr",
  "scripts": {
    "prettier": "prettier '**/*.+(css|scss)'",
    "prettier-write": "prettier --write '**/*.+(css|scss)'",
    "sass": "node-sass --include-path node_modules scss/_main.scss ../backend/funmooc/static/richie/css/main.css",
    "sass-production": "node-sass --include-path node_modules --output-style compressed scss/_main.scss ../backend/funmooc/static/richie/css/main.css",
    "watch-sass": "nodemon -e scss -x 'yarn sass'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openfun/fun-mooc.git"
  },
  "keywords": [
    "MOOC",
    "CMS"
  ],
  "author": {
    "name": "Open FUN (France Université Numérique)",
    "email": "fun.dev@fun-mooc.fr"
  },
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/openfun/fun-mooc/issues"
  },
  "homepage": "https://github.com/openfun/fun-mooc#readme",
  "dependencies": {},
  "devDependencies": {
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "richie-education": "1.14.1"
  }
}
